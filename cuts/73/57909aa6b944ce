mcweight * 'weight'
leptons && 'lep1_corrpt > 20'
    'lep2_corrpt > 20'
    '(lep1_q + lep2_q) == 0'
    'dilep_corr_pt > 30'
    'lep1_ismuon'
    'lep2_ismuon'
