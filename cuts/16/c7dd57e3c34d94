alpha_2 && 'alpha < 0.185'
    !alpha_1
LABEL: 0.155 #leq #alpha < 0.185
alpha_3 && 'alpha < 0.23'
    !alpha_1
    !alpha_2
LABEL: 0.185 #leq #alpha < 0.23
alpha_4 && !alpha_1
    !alpha_2
    !alpha_3
LABEL: 0.23 #leq #alpha < 0.3
smearplot_1 && smearplot
    alpha_1
smearplot_2 && smearplot
    alpha_2
smearplot_3 && smearplot
    alpha_3
smearplot_4 && smearplot
    alpha_4
dilep_m && 'dilep_m > 71'
    'dilep_m < 111'
rho_1 && 'rhoAll < 19'
LABEL: #rho < 19
rho_2 && !rho_1
LABEL: 19 #leq #rho
rhoplot_1_1 && smearplot_1
    rho_1
