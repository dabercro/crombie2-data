    jet
    'jet2'
    dilep_m
    'num_bjet == 2'
trig && '1'
jet && 'fabs(jet1_eta) < 2.4'
    'alpha < 0.3'
    'jet1_dphi_ll > 2.8'
    'jet1_btagDeepB > 0.7527'
alpha_1 && 'alpha < 0.155'
