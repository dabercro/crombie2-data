mcweight * 'genWeight'
    'bsf_DeepCSVB'
    'sf_pu'
leptons && 'lep1_pt > 20'
    'lep2_pt > 20'
    '(lep1_q + lep2_q) == 0'
iso && 'lep1_miniPFRelIso_all < 0.25'
    'lep2_miniPFRelIso_all < 0.25'
leppeak && leptons
    jet
trig && 'trigger'
jet && 'jet2_btagDeepB > 0.8'
    'jet1_btagDeepB > 0.8'
met && 'pfmet > 170'
    '!lep1'
metpeak && jet
    met
notrig && '1'
: leppeak trig mcweight , 
: metpeak notrig mcweight , 
