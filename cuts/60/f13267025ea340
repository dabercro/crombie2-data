mcweight * 'weight'
    'beff_sf_3'
leptons && 'lep1_corrpt > 20'
    'lep2_corrpt > 20'
    '(lep1_q + lep2_q) == 0'
    'dilep_corr_pt > 100'
    'lep1_ismuon'
    'lep2_ismuon'
iso && 'lep1_minireliso < 0.25'
    'lep2_minireliso < 0.25'
smearplot && leptons
    iso
    jet
trig && 'trigger_mu'
jet && 'fabs(jet1_eta) < 2.0'
    'jet2_pt/dilep_corr_pt < 0.3'
    'jet1_dphi_ll > 2.8'
    'jet1_btagDeepB > 0.9'
alpha_1 && 'jet2_pt/dilep_corr_pt < 0.15'
    'jet2'
alpha_2 && 'jet2_pt/dilep_corr_pt < 0.185'
    !alpha_1
alpha_3 && 'jet2_pt/dilep_corr_pt < 0.3'
    !alpha_1
    !alpha_2
smearplot_1 && alpha_1
    smearplot
smearplot_2 && alpha_2
    smearplot
smearplot_3 && alpha_3
    smearplot
rho_0 && 'fabs(jet1_eta) < 1.4'
rho_1 && !rho_0
rhoplot_1_0 && smearplot_1
    rho_0
rhoplot_2_0 && smearplot_2
    rho_0
rhoplot_3_0 && smearplot_3
    rho_0
rhoplot_1_1 && smearplot_1
    rho_1
rhoplot_2_1 && smearplot_2
    rho_1
rhoplot_3_1 && smearplot_3
    rho_1
