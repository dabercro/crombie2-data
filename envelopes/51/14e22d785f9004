ps_weight :
ps_weight_0 * (1 + (LHE_weight_orig - 1) * (genWeight<=1 && genWeight > 0.98))
ps_weight_1 * (1 + (LHE_weight_orig - 1) * (genWeight<=1 && genWeight > 0.98))
ps_weight_2 * (1 + (LHE_weight_orig - 1) * (genWeight<=1 && genWeight > 0.98))
ps_weight_3 * (1 + (LHE_weight_orig - 1) * (genWeight<=1 && genWeight > 0.98))
